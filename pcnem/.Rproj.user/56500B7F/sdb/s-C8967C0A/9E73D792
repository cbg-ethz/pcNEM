.Rdata\")\ndata_norm_plate2 <- extractNormWellFeatrues(\"G12\",data1,1:9)\ndata_norm_plate2_im5 <- extractNormWellFeatrues(\"G12\",data1,5)\n\npar(mfrow=c(1,2))\nhist(data_norm_plate1[,1],breaks=500)\nhist(data_norm_plate2[,1],breaks=500)\n\npval_norm <- NULL\npval_norm_im5 <- NULL\nfor(i in 1:ncol(data_norm_plate1)){pval_norm <- c(pval_norm,wilcox.test(data_norm_plate1[,i],data_norm_plate2[,i])$p.value)\n                                   pval_norm_im5 <- c(pval_norm_im5,wilcox.test(data_norm_plate1_im5[,i],data_norm_plate2_im5[,i])$p.value)}\nhist(pval_norm,breaks=100)\nhist(pval_norm_im5,breaks = 100)\n\nnames(pval_norm) <- colnames(data_norm_plate1)\ntemp <- pval_norm[!is.na(pval_norm)]\nsum(temp <= 0.01)\nsig_norm <- which(pval_norm > 0.01)\n\nnames(pval_norm_im5) <- colnames(data_norm_plate1_im5)\ntemp <- pval_norm_im5[!is.na(pval_norm_im5)]\nsum(temp <= 0.01)\nsig_norm_im5 <- which(pval_norm_im5 > 0.01)\n\n###### pval comparing any two random wells   ###########\ndata <- \n",
